{
    "configurations": 
    [
        {
            "name": "MSYS2",
            "includePath": 
            [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include"
            ],
            "defines": 
            [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "windows-gcc-x64",
            "compilerPath": "C:/msys64/ucrt64/bin/gcc.exe",
            "browse": 
            {
                "path": 
                [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}
